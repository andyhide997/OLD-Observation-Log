  <DataSets>
    <DataSet Name="ObservationSummary">
      <Query>
        <DataSourceName>Buses</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        YEAR(ObservationDate) AS Year, MONTH(ObservationDate) AS Month, COUNT(ObservationDate) AS Total, Company
FROM            Observations
WHERE        (Company IN (@Company))
GROUP BY YEAR(ObservationDate), MONTH(ObservationDate), Company
ORDER BY Year DESC, Month</CommandText>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Company">
          <DataField>Company</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CompanyUnique">
      <Query>
        <DataSourceName>Buses</DataSourceName>
        <CommandText>SELECT DISTINCT Company
FROM            Observations</CommandText>
      </Query>
      <Fields>
        <Field Name="Company">
          <DataField>Company</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
